set scf_type pk
set puream false
set mp2_type conv
set e_convergence 10
set d_convergence 10
e, wfn = gradient('hf', return_wfn=True, dertype='gradient')
e, mp2wfn = gradient('mp2', return_wfn=True, dertype='gradient')

print(mp2wfn.energy())
print(np.asarray(wfn.gradient()))
print(np.asarray(mp2wfn.gradient()))

mol = wfn.molecule()
bs = wfn.basisset()
basis = psi4.core.BasisSet.build(mol)
mints = psi4.core.MintsHelper(basis)
D = np.asarray(wfn.Da())
np.save('D2', D)
C = np.asarray(wfn.Ca())
print(C)
np.save('C2', C)
eps = np.asarray(wfn.epsilon_a())
print(eps)
np.save('eps2', eps)

F = np.asarray(wfn.Fa())
np.save('F2', F)

